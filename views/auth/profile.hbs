  <nav class="profile-navbar">
    <div class="title">
      <a href="/tattoo/upload"><img class="profile-icon" src="/images/uploadPhoto-C.png" alt="upload-photo"></a>
    </div>
    <div class="ham">
      <span class="bar1"></span>
      <span class="bar2"></span>
      <span class="bar3"></span>
    </div>
    <ul class="nav-sub">
      <li class="list-item"><a href="/users/profile/edit" class="nav-link">Edit profile</a></li>
      <li class="list-item"><a href="/users/unsubscribe" class="nav-link">Unsubscribe</a></li>
      <li class="list-item"><a href="/auth/logout" class="nav-link">Log out</a></li>
    </ul>
  </nav>
  <section class="main-section">
    <div class="profile-main-container">
      <section class="profile-image-container">
        {{#if user.profileImage}}
        <div class="user-info-div">
          <img src={{user.profileImage}} alt="{{user.username}}"/>
        </div>
        {{/if}}
        <p>{{toUpperCase user.username}}</p>
      </section>
      <section class="profile-info-container">
        {{#if user.city}}
        <div class="user-info-div">
          <p>{{user.city}}</p>
        </div>
        {{/if}}
        {{#if user.tattooNumber}}
        <div class="user-info-div">
          <p>I have {{user.tattooNumber}} tattoos</p>
        </div>
        {{/if}}
        {{#if user.profileDescription}}
        <div class="user-info-div">
          <p>About me: {{user.profileDescription}}</p>
        </div>
        {{/if}}
        {{#if tattooerUser}}
          {{#if user.studio}}
          <div class="user-info-div">
            <p>I work in: {{user.studio}}</p>
          </div>
          {{/if}}
          {{#if user.nextJourneys}}
          <div class="user-info-div">
            <p>Soon I will be in: {{user.nextJourneys}}</p>
          </div>
          {{/if}}
          {{#if user.tattooStyle}}
            <p>I work these styles:</p>
              <ul>
          {{#each user.tattooStyle}}
                <li>- {{this}} </li>
          {{/each}}
              </ul>
          {{/if}}
        {{/if}}
        {{#if tattooerUser}}
        <a href="/reviews/{{user._id}}">Reviews</a>
        {{/if}}
      </section>
      {{#each dataUser}}
      <img class="profile-photos" src={{tattooImage}} alt="tattoo">
      <p>{{year}}</p>
      <p>{{tattooer}}</p>
      <p>{{place}}</p>
        {{#each tattooPhotoStyle}}
        <p> - {{this}}</p>
        {{/each}}
      <a href="/tattoo/edit/{{_id}}">Edit</a>
      {{/each}}
  </div>
</section>